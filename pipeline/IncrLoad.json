				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "select * from File_1\nwhere Id > '@{activity('maxidcontroldata').output.firstRow.maxidcontrol}'\nAnd Id<= '@{activity('MaxSource').output.firstRow.maxidsource}'",
